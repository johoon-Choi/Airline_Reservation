<h1>{{ title }}</h1>
<table>
  <tr>
    <td>Flight_number</td>
    <td>Airline</td>
    <td>Weekdays</td>
  </tr>
  {{#each FLIGHT}}
    <form method="post">
      <tr>
        <td>{{Flight_number}}</td>
        <td>{{Airline}}</td>
        <td>{{Weekdays}}</td>
        <td style="border: none; margin-left: 10px;">
          <button 
            style="margin-left: 10px;"
            type="submit" 
            name="delBtn"
            value={{Flight_number}} 
            formaction="/AdminDeleteFLIGHT">
          삭제
          </button>
        </td>
      </tr>    
    </form>
  {{/each}}
</table>